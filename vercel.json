{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/spx/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/stock/[symbol]/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/spx/refresh/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/spy/holdings/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}

